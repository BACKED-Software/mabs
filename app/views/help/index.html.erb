<% content_for(:title, 'Help') %>
<div class="container mt-5" style="height: calc(100vh - 110px)">
    <h1>Help Articles</h1>
    <div class="list-group">
        <% @articles.each do |article| %>
        <%= link_to article.titleize, help_article_path(id: article), class: "list-group-item list-group-item-action" %>
        <% end %>
    </div>
</div>
