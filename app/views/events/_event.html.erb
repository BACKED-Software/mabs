<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong> <%= event.eventName %><br>
    <strong>Location:</strong> <%= event.eventLocation %><br>
    <strong>Time:</strong> <%= event.eventTime %><br>
    <strong>Info:</strong> <%= event.eventInfo %><br>
    <strong>Points:</strong> <%= event.eventPoints %><br>
    <strong>Sponsor Title:</strong> <%= event.sponsor_title %><br>
    <strong>Sponsor Description:</strong> <%= event.sponsor_description %><br>
    <% if user_signed_in? && @current_user.is_admin == true %>
      <strong>Password:</strong> <%= event.password %><br>
    <% end %>
  </p>
</div>
